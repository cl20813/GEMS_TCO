<Fit gneiting>
nano testing3.sh               # open a new text editor

'''
#!/bin/bash
#SBATCH --job-name=testing3       # Job name
#SBATCH --output=/home/jl2815/GEMS/testing3_%j.out            # Standard output file (%j = JobID)
#SBATCH --error=/home/jl2815/GEMS/testing3%j.err              # Standard error file (%j = JobID)
#SBATCH --time=24:00:00                   # Maximum time 
#SBATCH --ntasks=1                        # Number of tasks
#SBATCH --cpus-per-task=32                 # Number of CPU cores per task
#SBATCH --mem=200G                          # Memory per node
#SBATCH --partition=main               # Partition to submit to

module purge                                     # unload every other environment to avoid conflict
module use /projects/community/modulefiles                  # without this, I can't load 2024.06-ts840
module load anaconda/2024.06-ts840 

# Initialize Conda
eval "$(conda shell.bash hook)"                  # Initialize Conda for SLURM environment
conda activate gems_tco
# Add the GEMS_TCO package directory to PYTHONPATH
export PYTHONPATH=$PYTHONPATH:/home/jl2815/tco/GEMS_TCO

echo "Current date and time: $(date)"         

## note that without nugget, singular problem
echo "fixing nugget 5 --key 1 --lat_idx 5,6,7 --rho 4 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1"
srun python /home/jl2815/tco/exercise/fit_gneiting_bylag_11_16.py --key 1 --lat_idx 5 --rho 4 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1
srun python /home/jl2815/tco/exercise/fit_gneiting_bylag_11_16.py --key 1 --lat_idx 6 --rho 4 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1
srun python /home/jl2815/tco/exercise/fit_gneiting_bylag_11_16.py --key 1 --lat_idx 7 --rho 4 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1

echo "fixing nugget 5 --key 1 --lat_idx 5,6,7 --rho 6"
srun python /home/jl2815/tco/exercise/fit_gneiting_bylag_11_16.py --key 1 --lat_idx 5 --rho 6 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1
srun python /home/jl2815/tco/exercise/fit_gneiting_bylag_11_16.py --key 1 --lat_idx 6 --rho 6 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1
srun python /home/jl2815/tco/exercise/fit_gneiting_bylag_11_16.py --key 1 --lat_idx 7 --rho 6 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1

echo "fixing nugget 5 --key 1 --lat_idx 5,6,7 --rho 8"
srun python /home/jl2815/tco/exercise/fit_gneiting_bylag_11_16.py --key 1 --lat_idx 5 --rho 8 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1
srun python /home/jl2815/tco/exercise/fit_gneiting_bylag_11_16.py --key 1 --lat_idx 6 --rho 8 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1
srun python /home/jl2815/tco/exercise/fit_gneiting_bylag_11_16.py --key 1 --lat_idx 7 --rho 8 --params 1 1 1 1 1 1 1 --mm_cond_number 10 --bounds 0.05 10 0.05 10 1.5 5 0 1 0 1 0.05 500 0 1


# Store the job ID in a variable
JOBID=$SLURM_JOBID

# Run sacct to get resource usage details
sacct --units=G --format=MaxRSS,MaxDiskRead,MaxDiskWrite,Elapsed,NodeList -j $JOBID
```

sbatch testing3.sh
