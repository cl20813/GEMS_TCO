nano purespace.sh                 # open a new text editor

'''
#!/bin/bash
#SBATCH --job-name=purespace       # Job name
#SBATCH --output=/home/jl2815/GEMS/purespace_%j.out            # Standard output file (%j = JobID)
#SBATCH --error=/home/jl2815/GEMS/purespace_%j.err              # Standard error file (%j = JobID)
#SBATCH --time=24:00:00                   # Maximum time 
#SBATCH --ntasks=1                        # Number of tasks
#SBATCH --cpus-per-task=32                 # Number of CPU cores per task
#SBATCH --mem=200G                          # Memory per node
#SBATCH --partition=mem               # Partition to submit to

# Load the Anaconda module to use srun 

module purge                                     # unload every other environment to avoid conflict
module use /projects/community/modulefiles                  # without this, I can't load 2024.06-ts840
module load anaconda/2024.06-ts840 

# Initialize Conda
eval "$(conda shell.bash hook)"                  # Initialize Conda for SLURM environment
conda activate gems_tco
# Add the GEMS_TCO package directory to PYTHONPATH
export PYTHONPATH=$PYTHONPATH:/home/jl2815/tco/GEMS_TCO

#
echo "Current date and time: $(date)"
# export OMP_NUM_THREADS=1

# rho 1: 20000 4: 1250 5: 800 6:556 7:409 8:313
# Run the Python script

# space matern
echo "rho 8 6 4 2  --key 1 --params params 81 1 8 4 --mm_cond_number 10 --bounds 0.05 100 0.05 100 0.05 20 0.05 20" 

srun python /home/jl2815/tco/exercise/fit_spacematern.py --key 1 --rho 8 --params 81 1 8 4 --mm_cond_number 10 --bounds 0.05 100 0.05 100 0.05 20 0.05 20 
srun python /home/jl2815/tco/exercise/fit_spacematern.py --key 1 --rho 6 --params 81 1 8 4 --mm_cond_number 10 --bounds 0.05 100 0.05 100 0.05 20 0.05 20
srun python /home/jl2815/tco/exercise/fit_spacematern.py --key 1 --rho 4 --params 81 1 8 4 --mm_cond_number 10 --bounds 0.05 100 0.05 100 0.05 20 0.05 20
srun python /home/jl2815/tco/exercise/fit_spacematern.py --key 1 --rho 2 --params 81 1 8 4 --mm_cond_number 10 --bounds 0.05 100 0.05 100 0.05 20 0.05 20
```

sbatch purespace.sh 




